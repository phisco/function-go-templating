{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "crossplane": {
    "fileMatch": ["(^|/)example/.*\\.ya?ml$"]
  },
  "packageRules": [
    {
      "matchManagers": ["crossplane"],
      "matchPackageNames": ["example/**"],
      "groupName": "examples"
    }
  ]
}
